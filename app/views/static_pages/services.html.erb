<div class="col-md-1" ></div>
<div class="col-md-10">
  <% @services.each do |service| %>
  <div class="container news">
    <div class="col-md-4" style="text-align: center;padding-top: 20px;padding-bottom: 20px;">
      <%= image_tag(service.service_image.serv.url) %>
    </div>
    <div class="col-md-8" style="padding-top: 20px;">
      <h4 style="color: black;"><%= service.name %></h4>
      <p class="news_decript"><%= service.description %></p>
    </div>
  </div>
  <% end %>
  <p><%= will_paginate @services , :previous_label => "", :next_label => "", style:"margin-left: 30px;color: #363636;font-size: medium;"%></p>
</div>
<div class="col-md-1"></div>